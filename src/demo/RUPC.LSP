 (defun setup1()
   (setq d/2 (/ d 2))
   (setq d0/2 (/ d0 2))
   (setq d2/2 (/ d2 2))
   (setq d3/2 (/ d3 2))
   (if (< l 0) (setq h (* -1 h)))
   (if (< l 0) (setq h0 (* -1 h0)))
   (setq l2 (+ l h0))
   (setq p1 (list (- x1 d/2) y1))
   (setq p2 (list (+ x1 d/2) y1))
   (setq p3 (list (+ x1 d/2) (+ y1 h)))
   (setq p4 (list (- x1 d/2) (+ y1 h)))
   (command "pline" p1 "w" "0" "0" p2 p3 p4 p1 "")

   (setq p5 (list (+ x1 d0/2) (+ y1 h)))
   (setq p6 (list (+ x1 d0/2) (+ y1 l)))
   (setq p7 (list (- x1 d0/2) (+ y1 l)))
   (setq p8 (list (- x1 d0/2) (+ y1 h)))
   (command "pline" p5 "w" "0" "0" p6 p7 p8 "")

   (setq p11 (list (+ x1 d3/2) (+ y1 l)))
   (setq p12 (list (+ x1 d2/2) (+ y1 l2)))
   (setq p13 (list (- x1 d2/2) (+ y1 l2)))
   (setq p14 (list (- x1 d3/2) (+ y1 l)))
   (command "pline" p11 "w" "0" "0" p12 p13 p14 "")
   (setup)
 )
   (setup)
