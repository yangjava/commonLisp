 (defun setup()
   (setq ch (getstring "\n,D(4)(5)(6)(8)(10)(12) ?"))

   (setq l (getreal "\n How about the RUPC longer ?"))

   (setq base (getpoint "\n Where will you put on the RUPC ?"))
   (setq x1 (eval (car base)))
   (setq y1 (eval (cadr base)))

   (setvar "cmdecho" 1)
   (while (or (eq ch "4") (eq ch "D4")) (D4))
   (while (or (eq ch "5") (eq ch "D5")) (D5))
   (while (or (eq ch "6") (eq ch "D6")) (D6))
   (while (or (eq ch "8") (eq ch "D8")) (D8))
   (while (or (eq ch "10") (eq ch "D10")) (D10))
   (while (or (eq ch "12") (eq ch "D12")) (D12))
 )

