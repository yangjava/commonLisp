(defun setup1()
  ;(if (< L 0) (setq B (* -1 B)))
   (if (< L 0) (setq D (* -1 D)))
   (if (< L 0) (setq H (* -1 H)))
   (setq B1 (/ B 2))
   (setq D1 (/ D 2))
