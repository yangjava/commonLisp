   (setq E (- L R))
   (setq F (- D1 R))

   (setq p1 (list (- x1 B1) y1))
   (setq p2 (list (+ x1 B1) y1))
   (setq p3 (list (+ x1 B1) (+ y1 H)))
   (setq p4 (list (- x1 B1) (+ y1 H)))
   (command "pline" p1 "w" "0" "0" p2 p3 p4 p1 "")

   (setq p5 (list (+ x1 D1) (+ y1 H)))
   (setq p6 (list (+ x1 D1) (+ y1 E)))
   (setq p7 (list (+ x1 F) (+ y1 L)))
   (setq p8 (list (- x1 F) (+ y1 L)))
   (setq p9 (list (- x1 D1) (+ y1 E)))
   (setq p10 (list (- x1 D1) (+ y1 H)))
   (command "pline" p5 p6 "A" p7 "L" p8 "A" p9 "L" P10 "")
